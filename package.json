{
  "name": "romi-dashboard",
  "version": "0.0.1-alpha.0",
  "private": true,
  "homepage": ".",
  "scripts": {
    "start": ". e2e/env.sh && concurrently npm:start:auth npm:start:rmf npm:start:react",
    "start:react-auth": ". e2e/env.sh && concurrently npm:start:auth npm:start:react",
    "start:docker": ". e2e/env.sh && scripts/dockert docker-compose -f docker/docker-compose.yml up dev",
    "start:auth": ". e2e/env.sh && scripts/dockert docker-compose -f docker/docker-compose.yml up auth",
    "start:react": ". e2e/env.sh && react-scripts start",
    "start:react:e2e": "serve -c ../e2e/serve.json build",
    "start:mock": "REACT_APP_MOCK=1 react-scripts start",
    "start:rmf": "ts-node -P e2e/tsconfig.json e2e/startBackend.ts",
    "build": "react-scripts build",
    "build:e2e": ". e2e/env.sh && npm run build",
    "build:storybook": "build-storybook -s public",
    "test": "react-scripts test",
    "test:coverage": "npm run test -- --coverage --watchAll=false",
    "test:e2e": ". e2e/env.sh && node scripts/test-e2e.js",
    "test:e2e:docker": ". e2e/env.sh && scripts/dockert docker-compose -f docker/docker-compose.yml up e2e",
    "test:e2e:wdio": "ts-node -P e2e/tsconfig.json e2e/auth-ready.ts && wdio",
    "test:e2e:dev": ". e2e/env.sh && ROMI_DASHBOARD_PORT=3000 ROMI_DASHBOARD_LAUNCH_MODE=none wdio",
    "sync:docker": "docker pull docker.pkg.github.com/osrf/romi-dashboard/e2e && docker pull quay.io/keycloak/keycloak:11.0.0",
    "eject": "react-scripts eject",
    "deploy:demo": "REACT_APP_MOCK=1 npm run build && NODE_DEBUG=gh-pages gh-pages -d build -m \"$(git rev-parse HEAD)\"",
    "setup": "node ./scripts/setup/setup.js && node ./scripts/setup/get-icons.js",
    "storybook": "start-storybook -p 9009 -s public -s src/stories/static",
    "build-storybook": "build-storybook -s public -s src/stories/static"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@osrf/romi-js-core-interfaces": "^0.1.1",
    "@osrf/romi-js-soss-transport": "^0.1.0",
    "@types/debug": "^4.1.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/leaflet": "^1.5.17",
    "@types/node": "12.12.14",
    "@types/react": "^16.9.49",
    "@types/react-leaflet": "^2.5.2",
    "@types/react-router": "^5.1.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/uuid": "^8.3.0",
    "axios": "^0.20.0",
    "chalk": "^4.1.0",
    "clsx": "^1.1.1",
    "debug": "^4.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "eventemitter3": "^4.0.7",
    "keycloak-js": "^11.0.2",
    "leaflet": "^1.7.1",
    "node-vibrant": "^3.1.5",
    "react": "^16.12.0",
    "react-async": "^10.0.0",
    "react-dom": "^16.12.0",
    "react-hotkeys": "^2.0.0",
    "react-leaflet": "^2.7.0",
    "react-leaflet-control": "^2.1.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "sweetalert2": "^10.0.2",
    "typeface-roboto": "0.0.75",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addons": "^6.0.21",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.21",
    "@types/jest": "^26.0.13",
    "@types/mocha": "^8.0.3",
    "@types/react-dom": "^16.9.8",
    "@wdio/cli": "^6.4.7",
    "@wdio/local-runner": "^6.1.19",
    "@wdio/mocha-framework": "^6.4.7",
    "@wdio/spec-reporter": "^6.4.7",
    "@wdio/sync": "^6.4.7",
    "chromedriver": "^85.0.1",
    "concurrently": "^5.3.0",
    "enzyme-to-json": "^3.5.0",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1",
    "serve": "^11.3.2",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.7",
    "wdio-chromedriver-service": "^6.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!src/app-config.ts",
      "!src/leaflet/**",
      "!src/components/auth/authenticator.ts",
      "!**/stories/**",
      "!**/mock/**"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
