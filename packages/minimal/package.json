{
  "name": "minimal",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "concurrently npm:start:rmf-server npm:start:rmf npm:start:react",
    "start:react": "react-scripts start",
    "start:rmf": "ts-node -P scripts/tsconfig.json -T scripts/start-rmf.ts",
    "start:rmf-server": "RMF_SERVER_USE_SIM_TIME=true npm --prefix ../api-server start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "setup": "node  ../rmf-tools/lib/scripts/setup/setup.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }, 
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.2.3",
    "@testing-library/react-hooks": "^5.0.3",
    "@testing-library/user-event": "^12.1.9",
    "@types/jest": "^26.0.13",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.12",
    "@types/react-router": "^5.1.7",
    "@types/react-router-dom": "^5.1.7",
    "api-client": "*",
    "api-server": "*",
    "concurrently": "^5.3.0",
    "react": "^16.13.1",
    "react-components": "*",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "rmf-auth": "*",
    "rmf-models": "*",
    "rmf-tools": "*",
    "ts-node": "^9.1.1",
    "typescript": "4.2.4",
    "web-vitals": "^1.1.2"
  }
}
