{
  "name": "rmf-dashboard",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "pnpm run --filter {.}^... build && node scripts/build.js",
    "build-storybook": "storybook build",
    "setup": "node ./scripts/setup/setup.js",
    "start": "concurrently npm:start:rmf-server npm:start:react",
    "start:airport": "RMF_DASHBOARD_DEMO_MAP=airport_terminal.launch.xml pnpm run start:sim",
    "start:clinic": "RMF_DASHBOARD_DEMO_MAP=clinic.launch.xml pnpm run start:sim",
    "start:react": "pnpm run --filter {.}^... build && node scripts/start.js",
    "start:rmf": "node scripts/start-rmf.js",
    "start:rmf-server": "RMF_SERVER_USE_SIM_TIME=true npm --prefix ../api-server start",
    "start:rmf-server:psql": "RMF_SERVER_USE_SIM_TIME=true npm run --prefix ../api-server start:psql",
    "start:sim": "concurrently npm:start:rmf-server npm:start:rmf npm:start:react",
    "storybook": "storybook dev -p 6006",
    "test": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cd e2e && npm test",
    "test:e2e:dev": "cd e2e && pnpm run test:dev"
  },
  "devDependencies": {
    "@aldabil/react-scheduler": "^2.6.7",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@fontsource/roboto": "^4.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@mui/icons-material": "^5.8.3",
    "@mui/material": "^5.8.3",
    "@mui/system": "^5.8.3",
    "@mui/x-date-pickers": "^5.0.20",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "@react-three/drei": "^9.103.0",
    "@react-three/fiber": "^8.16.1",
    "@storybook/addon-essentials": "^8.0.5",
    "@storybook/addon-interactions": "^8.0.5",
    "@storybook/addon-links": "^8.0.5",
    "@storybook/addon-onboarding": "^8.0.5",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.3",
    "@storybook/blocks": "^8.0.5",
    "@storybook/react": "^8.0.5",
    "@storybook/react-webpack5": "^8.0.5",
    "@storybook/test": "^8.0.5",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/debug": "^4.1.5",
    "@types/jest": "^29.5.12",
    "@types/leaflet": "^1.5.17",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-grid-layout": "^1.3.2",
    "@types/react-leaflet": "^2.5.2",
    "@types/three": "^0.156.0",
    "ajv": "^8.10.0",
    "api-client": "workspace:*",
    "api-server": "file:../api-server",
    "axios": "^1.6.8",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.0.1",
    "bfj": "^7.0.2",
    "browserslist": "^4.18.1",
    "camelcase": "^6.2.1",
    "canvas": "^2.11.2",
