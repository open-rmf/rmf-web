{
  "name": "romi-dashboard",
  "version": "0.0.1-alpha.0",
  "private": true,
  "scripts": {
    "start": "concurrently npm:start:ros2-bridge npm:start:rmf npm:start:react",
    "start:react": "react-scripts start",
    "start:rmf": "ts-node -P scripts/tsconfig.json -T scripts/start-rmf.ts",
    "start:ros2-bridge": "ros2-bridge --port 50002",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "npm run test -- --coverage --watchAll=false",
    "test:e2e": "cd e2e && npm test",
    "test:e2e:dev": "cd e2e && npm run test:dev",
    "eject": "react-scripts eject",
    "setup": "node ./scripts/setup/setup.js && node ./scripts/setup/get-icons.js",
    "storybook": "../../node_modules/.bin/start-storybook -p 9009 -s public -s src/stories/static",
    "build:storybook": "build-storybook -s public -s src/stories/static"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@msgpack/msgpack": "^2.1.0",
    "@osrf/romi-js-core-interfaces": "^0.1.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react-hooks": "^3.4.2",
    "@testing-library/user-event": "^12.1.9",
    "@types/debug": "^4.1.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/leaflet": "^1.5.17",
    "@types/node": "12.12.14",
    "@types/react": "^16.9.49",
    "@types/react-leaflet": "^2.5.2",
    "@types/react-router": "^5.1.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/reactour": "^1.17.1",
    "@types/uuid": "^8.3.0",
    "axios": "^0.21.1",
    "api-client": "file:../api-client",
    "chalk": "^4.1.0",
    "clsx": "^1.1.1",
    "eventemitter3": "^4.0.7",
    "keycloak-js": "^11.0.2",
    "leaflet": "^1.7.1",
    "node-vibrant": "^3.1.5",
    "react-async": "^10.0.0",
    "react-components": "file:../react-components",
    "react-hotkeys": "^2.0.0",
    "react-leaflet": "^2.7.0",
    "react-leaflet-control": "^2.1.1",
    "react-router": "^5.2.0",
    "react-scripts": "^4.0.1",
    "reactour": "^1.18.0",
    "rmf-auth": "file:../rmf-auth",
    "styled-components": "^4.4.1",
    "typeface-roboto": "0.0.75",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.3",
    "@types/jest": "^26.0.13",
    "@types/react-dom": "^16.9.8",
    "canvas": "^2.6.1",
    "concurrently": "^5.3.0",
    "enzyme-to-json": "^3.5.0",
    "gh-pages": "^3.1.0",
    "react-test-renderer": "^17.0.1",
    "ros2-bridge": "file:../ros2-bridge",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.7"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.tsx",
      "!src/rpc-client.ts",
      "!src/ros2-transport.ts",
      "!src/serviceWorker.ts",
      "!src/app-config.ts",
      "!src/leaflet/**",
      "!src/components/auth/**",
      "!src/managers/**",
      "!src/components/rmf-app/**",
      "!**/stories/**",
      "!**/tests/**",
      "!src/components/__trash/**",
      "!src/components/dashboard/dashboard.tsx",
      "!src/components/dashboard/index.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
