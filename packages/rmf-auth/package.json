{
  "name": "rmf-auth",
  "version": "0.0.1",
  "description": "Auth layer to be used on web UI services",
  "main": "lib/index.js",
  "scripts": {
    "start": "../../node_modules/.bin/start-storybook -p 6006 -s stories",
    "prepare": "npm run build",
    "build": "npm run lint && tsc --build",
    "build:watch": "tsc --build --watch",
    "build:storybook": "tsc --build stories && ../../node_modules/.bin/build-storybook -s stories",
    "clean": "tsc --build --clean",
    "test": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "../../node_modules/.bin/eslint --ext ts,tsx, lib stories tests"
  },
  "keywords": [
    "auth",
    "rmf"
  ],
  "author": "matiasbavera@gmail.com",
  "license": "Apache-2.0",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react-hooks": "^3.4.2",
    "@testing-library/user-event": "^12.1.9",
    "@types/debug": "^4.1.5",
    "@types/node": "12.12.14",
    "@types/react": "^16.9.49",
    "@types/react-router": "^5.1.7",
    "@types/react-router-dom": "^5.1.5",
    "axios": "^0.21.1",
    "debug": "^4.1.1",
    "eventemitter3": "^4.0.7",
    "keycloak-js": "^11.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^4.4.1",
    "typeface-roboto": "0.0.75"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.3",
    "@types/jest": "^26.0.13",
    "@types/react-dom": "^16.9.8",
    "concurrently": "^5.3.0",
    "gh-pages": "^3.1.0",
    "react-test-renderer": "^17.0.1",
    "ts-node": "^9.0.0",
    "jest": "^26.5.0",
    "ts-jest": "^26.4.1",
    "typescript": "^3.9.7",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "react": "^16.13.1"
  },
  "files": [
    "lib/"
  ],
  "jest": {
    "collectCoverageFrom": [
      "lib/**/*.{js,jsx,ts,tsx}",
      "!lib/index.tsx",
      "!**/stories/**",
      "!**/tests/**"
    ]
  }
}
