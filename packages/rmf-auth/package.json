    "concurrently": "^5.3.0",
    "eslint": "^7.26.0",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "jest": "^26.5.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.1.1",
