{
  "name": "rmf-auth",
  "version": "0.0.1",
  "description": "Auth layer to be used on web UI services",
  "main": "index.js",
  "scripts": {
    "start": "start-storybook -p 6006 -s test-data",
    "prepare": "tsc --build && cp package.json dist/",
    "prepack": "npm run lint && npm run test",
    "build:watch": "tsc --build --watch",
    "build:storybook": "build-storybook -s test-data",
    "clean": "tsc --build --clean",
    "test": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint --ext ts,tsx, lib"
  },
  "keywords": [
    "auth",
    "rmf"
  ],
  "author": "matiasbavera@gmail.com",
  "license": "Apache-2.0",
  "publishConfig": {
    "directory": "dist"
  },
  "dependencies": {
